class ConstOverload
{
    void area(float x)
    {
        System.out.println("the area of the square is "+Math.pow(x, 2)+" sq units");
    }
    void area(float x, float y)
    {
        System.out.println("the area of the rectangle is "+x*y+" sq units");
    }
    
//two methods with same name but differnt number of arguments.

     public static void main(String args[]) 
	{
	   ConstOverload ob = new ConstOverload();// creating instance
	   ob.area(5);   
	   ob.area(11,12);
   }//depending upon number of arguments provided the method to be run is selected.
}
